{% extends 'base.html.twig' %}

{% block title %}Site{% endblock %}

{% block body %}
    <h1>Site</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ site.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ site.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ site.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ site.city }}</td>
            </tr>
            <tr>
                <th>ContactName</th>
                <td>{{ site.contactName }}</td>
            </tr>
            <tr>
                <th>ContactPhone</th>
                <td>{{ site.contactPhone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_site_index') }}">back to list</a>

    <a href="{{ path('app_site_edit', {'id': site.id}) }}">edit</a>

    {{ include('site/_delete_form.html.twig') }}
{% endblock %}
